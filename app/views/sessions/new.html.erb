<%= form_for :session do |f| %>
  
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email, class: 'form-control'%>
  </div>
  
  <div class="form-group">
    <%= f.label :password %>
    <%= f.text_field :password, class: 'form-control' %>
  </div>
  <button type="submit" class="btn btn-default">Sign In</button>

<% end %>